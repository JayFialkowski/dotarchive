{
  "manifest_version": 2,

  "name": "DotArchive",
  "description": "This extension will provide recent professional DotA matches, as well as links to their VODs.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "dotarchive.html",
    "default_title": "DotArchive"
  },
  "permissions": [ 
    "http://www.gosugamers.net/dota2/gosubet"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://maxcdn.bootstrapcdn.com; object-src 'self'"
}